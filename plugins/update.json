{
  "plugins": [
    {
      "id":"truthfacts",
      "stripSource":"http://truthfacts.com/",
      "extractJson":"function(page) {var regex = \/<img[^>]*data-src=\"([^\"]*)\"\/;var match = regex.exec(page);return match[1];}"
    },
    {
      "id":"suporte",
      "stripSource":"https://vidadesuporte.com.br/",
      "extractJson":"function(page) {var regex = \/<img[^>]* src=\"([^\"]+\\/wp-content\\/uploads\\/[^\"]*suporte[^\"]+)\"\/;var match = regex.exec(page);return match[1];}"
    }
  ]
}
